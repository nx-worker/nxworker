{
  "name": "nxworker",
  "description": "Monorepo for the @nxworker NPM organization.",
  "private": true,
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "nx": "nx",
    "start": "nx serve examples-angular-nrwl-airlines-booking-desktop-app",
    "build": "nx run-many --target=build --configuration=production --all --parallel=false",
    "test": "nx run-many --target=test --all",
    "lint": "nx workspace-lint && nx run-many --target=lint --all",
    "e2e": "nx run-many --target=e2e --all --parallel=false",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build --parallel=false",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "contributors:add": "all-contributors add",
    "format": "pnpm sort-projects && nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "hooks:pre-commit": "node hooks/pre-commit.js",
    "commit": "git-cz",
    "help": "nx help",
    "sort-projects": "json -I -f workspace.json -e \"this.projects = Object.fromEntries(Object.entries(this.projects).sort(([a], [b]) => a.localeCompare(b)));\" && json -I -f nx.json -e \"this.projects = Object.fromEntries(Object.entries(this.projects).sort(([a], [b]) => a.localeCompare(b)));\"",
    "ci": "pnpm lint && pnpm test && pnpm build && pnpm e2e",
    "affected:ci": "pnpm affected:lint && pnpm affected:test && pnpm affected:build && pnpm affected:e2e"
  },
  "dependencies": {
    "@angular/animations": "~12.0.0",
    "@angular/common": "~12.0.0",
    "@angular/compiler": "~12.0.0",
    "@angular/core": "~12.0.0",
    "@angular/forms": "~12.0.0",
    "@angular/platform-browser": "~12.0.0",
    "@angular/platform-browser-dynamic": "~12.0.0",
    "@angular/router": "~12.0.0",
    "@ngrx/effects": "~12.0.0",
    "@ngrx/entity": "~12.0.0",
    "@ngrx/router-store": "~12.0.0",
    "@ngrx/store": "~12.0.0",
    "@nrwl/angular": "~12.3.5",
    "luxon": "^1.25.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.0",
    "@angular-eslint/eslint-plugin": "~12.0.0",
    "@angular-eslint/eslint-plugin-template": "~12.0.0",
    "@angular-eslint/template-parser": "~12.0.0",
    "@angular/cli": "~12.0.0",
    "@angular/compiler-cli": "~12.0.0",
    "@angular/language-service": "~12.0.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-angular": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@ngrx/schematics": "~12.0.0",
    "@ngrx/store-devtools": "~12.0.0",
    "@nrwl/cli": "~12.3.5",
    "@nrwl/cypress": "~12.3.5",
    "@nrwl/devkit": "~12.3.5",
    "@nrwl/eslint-plugin-nx": "~12.3.5",
    "@nrwl/jest": "~12.3.5",
    "@nrwl/node": "~12.3.5",
    "@nrwl/nx-cloud": "^12.1.3",
    "@nrwl/nx-plugin": "~12.3.5",
    "@nrwl/tao": "~12.3.5",
    "@nrwl/workspace": "~12.3.5",
    "@types/jest": "~26.0.8",
    "@types/luxon": "^1.25.0",
    "@types/node": "^14.14.33",
    "@typescript-eslint/eslint-plugin": "~4.19.0",
    "@typescript-eslint/parser": "~4.19.0",
    "all-contributors-cli": "^6.19.0",
    "cypress": "^7.4.0",
    "dotenv": "~8.2.0",
    "eslint": "~7.10.0",
    "eslint-config-prettier": "~8.1.0",
    "eslint-plugin-cypress": "^2.11.2",
    "git-cz": "^4.7.6",
    "husky": "^4.3.7",
    "jest": "~26.2.2",
    "jest-preset-angular": "~8.4.0",
    "json": "^10.0.0",
    "lint-staged": "^10.5.3",
    "ng-packagr": "~12.0.2",
    "prettier": "~2.2.1",
    "ts-jest": "~26.5.5",
    "ts-node": "~9.1.1",
    "typescript": "~4.2.4"
  },
  "volta": {
    "node": "14.17.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,ts,html,component.html}": [
      "pnpm format:write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "pnpm hooks:pre-commit && lint-staged"
    }
  }
}
