{
  "name": "nxworker",
  "description": "Monorepo for the @nxworker NPM organization.",
  "private": true,
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "preinstall": "(pnpm remove codelyzer || echo ✅ Codelyzer is already removed.) && (pnpm remove tslint || echo ✅ TSLint is already removed.)",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx",
    "start": "nx serve",
    "build": "nx run-many --target=build --configuration=production --projects=angular --with-deps --parallel",
    "build-examples": "nx run-many --target=build --configuration=production --projects=examples-angular-nrwl-airlines-booking-desktop-app,examples-angular-nrwl-airlines-booking-mobile-app,examples-angular-nrwl-airlines-check-in-desktop-app,examples-angular-nrwl-airlines-check-in-mobile-app --with-deps",
    "test": "nx run-many --target=test --projects=angular --with-deps --parallel",
    "test-examples": "nx run-many --target=test --projects=examples-angular-nrwl-airlines-booking-desktop-app,examples-angular-nrwl-airlines-booking-mobile-app,examples-angular-nrwl-airlines-check-in-desktop-app,examples-angular-nrwl-airlines-check-in-mobile-app --parallel",
    "lint": "nx workspace-lint && nx run-many --target=lint --projects=angular --parallel",
    "lint-examples": "nx workspace-lint && nx run-many --target=lint --projects=examples-angular-nrwl-airlines-booking-desktop-app,examples-angular-nrwl-airlines-booking-mobile-app,examples-angular-nrwl-airlines-check-in-desktop-app,examples-angular-nrwl-airlines-check-in-mobile-app --parallel",
    "e2e": "nx run-many --target=e2e --all",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write && pnpx prettier --config .prettierrc --write \"**/*.*\" \"!dist/**\" \"!pnpm-lock.yaml\"",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "sort-projects": "json -I -f workspace.json -e \"this.projects = Object.fromEntries(Object.entries(this.projects).sort(([a], [b]) => (a < b) ? -1 : (a > b) ? 1 : 0));\" && json -I -f nx.json -e \"this.projects = Object.fromEntries(Object.entries(this.projects).sort(([a], [b]) => (a < b) ? -1 : (a > b) ? 1 : 0));\"",
    "ci": "pnpm lint && pnpm test && pnpm build && pnpm lint-examples && pnpm test-examples && pnpm e2e && pnpm build-examples",
    "affected:ci": "pnpm affected:lint && pnpm affected:test && pnpm affected:build && pnpm affected:e2e"
  },
  "dependencies": {
    "@angular/animations": "^11.0.0",
    "@angular/common": "^11.0.0",
    "@angular/compiler": "^11.0.0",
    "@angular/core": "^11.0.0",
    "@angular/forms": "^11.0.0",
    "@angular/platform-browser": "^11.0.0",
    "@angular/platform-browser-dynamic": "^11.0.0",
    "@angular/router": "^11.0.0",
    "@ngrx/effects": "^10.1.2",
    "@ngrx/entity": "^10.1.2",
    "@ngrx/router-store": "^10.1.2",
    "@ngrx/store": "^10.1.2",
    "@nrwl/angular": "11.0.20",
    "luxon": "^1.25.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.1",
    "@angular-eslint/eslint-plugin": "1.0.0",
    "@angular-eslint/eslint-plugin-template": "1.0.0",
    "@angular-eslint/template-parser": "1.0.0",
    "@angular/cli": "^11.0.0",
    "@angular/compiler-cli": "^11.0.0",
    "@angular/language-service": "^11.0.0",
    "@ngrx/schematics": "^10.1.2",
    "@ngrx/store-devtools": "^10.1.2",
    "@nrwl/cli": "11.0.20",
    "@nrwl/cypress": "11.0.20",
    "@nrwl/devkit": "11.0.20",
    "@nrwl/eslint-plugin-nx": "11.0.20",
    "@nrwl/jest": "11.0.20",
    "@nrwl/node": "11.0.20",
    "@nrwl/nx-cloud": "10.1.10",
    "@nrwl/nx-plugin": "11.0.20",
    "@nrwl/tao": "11.0.20",
    "@nrwl/workspace": "11.0.20",
    "@types/jest": "26.0.8",
    "@types/luxon": "^1.25.0",
    "@types/node": "12.12.38",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "cypress": "^5.5.0",
    "dotenv": "6.2.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.13",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "jest": "26.2.2",
    "jest-preset-angular": "8.3.1",
    "json": "^10.0.0",
    "prettier": "2.1.2",
    "ts-jest": "26.4.0",
    "ts-node": "~9.1.1",
    "typescript": "~4.0.3"
  },
  "volta": {
    "node": "12.20.1"
  }
}
