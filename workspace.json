{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "pnpm"
  },
  "generators": {
    "@nrwl/angular:application": {
      "e2eTestRunner": "cypress",
      "linter": "eslint",
      "routing": false,
      "strict": true,
      "style": "scss",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "displayBlock": true,
      "skipTests": true,
      "style": "scss"
    },
    "@nrwl/angular:library": {
      "linter": "eslint",
      "strict": true,
      "style": "scss",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:storybook-configuration": {
      "linter": "eslint"
    }
  },
  "projects": {}
}
