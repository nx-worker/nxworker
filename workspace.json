{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "pnpm"
  },
  "generators": {
    "@nrwl/angular": {
      "application": {
        "e2eTestRunner": "cypress",
        "linter": "eslint",
        "routing": false,
        "strict": true,
        "style": "scss",
        "unitTestRunner": "jest"
      },
      "component": {
        "displayBlock": true,
        "skipTests": true,
        "style": "scss"
      },
      "library": {
        "linter": "eslint",
        "strict": true,
        "style": "scss",
        "unitTestRunner": "jest"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    }
  },
  "projects": {}
}
